v0xae8ba0_0 .net "sIn", 0 0, v0xae8650_0; 1 drivers
v0xae8c70_0 .net "sOut", 0 0, v0xae80e0_0; 1 drivers
S_0xae81d0 .scope module, "prueba" "tester" 2 5, 3 1, S_0xad0d50;
 .timescale 0 0;
v0xae82c0_0 .var "D", 3 0;
v0xae8360_0 .var "clk", 0 0;
v0xae8410_0 .var "dir", 0 0;
v0xae84c0_0 .var "enb", 0 0;
v0xae85a0_0 .var "modo", 1 0;
v0xae8650_0 .var "sIn", 0 0;
S_0xacc4f0 .scope module, "registro" "RegDesp" 2 6, 4 1, S_0xad0d50;
 .timescale 0 0;
v0xacc5e0_0 .alias "D", 3 0, v0xae86d0_0;
v0xae7cd0_0 .var "Q", 3 0;
v0xae7d70_0 .alias "clk", 0 0, v0xae8820_0;
v0xae7e10_0 .alias "dir", 0 0, v0xae88f0_0;
v0xae7ec0_0 .alias "enb", 0 0, v0xae89c0_0;
v0xae7f60_0 .alias "modo", 1 0, v0xae8a90_0;
v0xae8040_0 .alias "sIn", 0 0, v0xae8ba0_0;
v0xae80e0_0 .var "sOut", 0 0;
E_0xad0c80 .event posedge, v0xae7d70_0;
    .scope S_0xae81d0;
T_0 ;
    %delay 2, 0;
    %load/v 8, v0xae8360_0, 1;
    %inv 8, 1;
    %set/v v0xae8360_0, 8, 1;
    %jmp T_0;
    .thread T_0;
    .scope S_0xae81d0;
T_1 ;
    %ix/load 0, 1, 0;
    %assign/v0 v0xae8360_0, 0, 0;
    %ix/load 0, 1, 0;
    %assign/v0 v0xae84c0_0, 0, 1;
    %ix/load 0, 1, 0;
    %assign/v0 v0xae8410_0, 0, 0;
    %ix/load 0, 1, 0;
    %assign/v0 v0xae8650_0, 0, 1;
    %ix/load 0, 2, 0;
    %assign/v0 v0xae85a0_0, 0, 0;
    %vpi_call 3 16 "$display", "Prueba 1; Rotacion a la Izquierda";
    %ix/load 0, 2, 0;
    %assign/v0 v0xae85a0_0, 0, 0;
    %ix/load 0, 1, 0;
    %assign/v0 v0xae8410_0, 0, 0;
    %movi 8, 1, 4;
    %ix/load 0, 4, 0;
    %assign/v0 v0xae82c0_0, 0, 8;
    %delay 16, 0;
    %vpi_call 3 22 "$display", "Prueba 5; Carga en Paralelo";
    %movi 8, 5, 4;
    %ix/load 0, 4, 0;
    %assign/v0 v0xae82c0_0, 0, 8;
    %movi 8, 2, 2;
    %ix/load 0, 2, 0;
    %assign/v0 v0xae85a0_0, 0, 8;
    %ix/load 0, 1, 0;
    %assign/v0 v0xae84c0_0, 0, 1;
    %delay 4, 0;
    %vpi_call 3 28 "$display", "Prueba 2; Rotacion a la Derecha";
    %ix/load 0, 2, 0;
    %assign/v0 v0xae85a0_0, 0, 0;
    %ix/load 0, 1, 0;
    %assign/v0 v0xae8410_0, 0, 1;
    %delay 16, 0;
    %vpi_call 3 33 "$display", "Prueba 5; Carga en Paralelo";
    %movi 8, 6, 4;
    %ix/load 0, 4, 0;
    %assign/v0 v0xae82c0_0, 0, 8;
    %movi 8, 2, 2;
    %ix/load 0, 2, 0;
    %assign/v0 v0xae85a0_0, 0, 8;
    %ix/load 0, 1, 0;
    %assign/v0 v0xae84c0_0, 0, 1;
    %delay 4, 0;
    %vpi_call 3 39 "$display", "Prueba 3; Rotacion Circular a la Izquierda";
    %movi 8, 1, 2;
    %ix/load 0, 2, 0;
    %assign/v0 v0xae85a0_0, 0, 8;
    %ix/load 0, 1, 0;
    %assign/v0 v0xae8410_0, 0, 0;
    %delay 16, 0;
    %vpi_call 3 44 "$display", "Prueba 5; Carga en Paralelo";
    %movi 8, 9, 4;
    %ix/load 0, 4, 0;
    %assign/v0 v0xae82c0_0, 0, 8;
    %movi 8, 2, 2;
    %ix/load 0, 2, 0;
    %assign/v0 v0xae85a0_0, 0, 8;
    %ix/load 0, 1, 0;
    %assign/v0 v0xae84c0_0, 0, 1;
    %delay 4, 0;
    %vpi_call 3 51 "$display", "Prueba 4; Rotacion Circular a la Derecha";
    %movi 8, 1, 2;
    %ix/load 0, 2, 0;
    %assign/v0 v0xae85a0_0, 0, 8;
    %ix/load 0, 1, 0;
    %assign/v0 v0xae8410_0, 0, 1;
    %delay 16, 0;
    %vpi_call 3 57 "$finish";
    %end;
    .thread T_1;
    .scope S_0xacc4f0;
T_2 ;
    %wait E_0xad0c80;
    %load/v 8, v0xae7ec0_0, 1;
    %jmp/0xz  T_2.0, 8;
    %load/v 8, v0xae7e10_0, 1;
    %jmp/0xz  T_2.2, 8;
    %load/v 8, v0xae7f60_0, 2;
    %cmpi/u 8, 0, 2;
    %jmp/0xz  T_2.4, 4;
    %ix/load 1, 1, 0;
    %mov 4, 0, 1;
    %jmp/1 T_2.6, 4;
    %load/x1p 12, v0xae7cd0_0, 3;
    %jmp T_2.7;
T_2.6 ;
    %mov 12, 2, 3;
T_2.7 ;
    %mov 8, 12, 3; Move signal select into place
    %load/v 11, v0xae8040_0, 1;
    %ix/load 0, 4, 0;
    %assign/v0 v0xae7cd0_0, 0, 8;
    %load/v 8, v0xae7cd0_0, 1; Only need 1 of 4 bits
; Save base=8 wid=1 in lookaside.
    %ix/load 0, 1, 0;
    %assign/v0 v0xae80e0_0, 0, 8;
    %jmp T_2.5;
T_2.4 ;
    %load/v 8, v0xae7f60_0, 2;
    %cmpi/u 8, 1, 2;
    %jmp/0xz  T_2.8, 4;
    %ix/load 1, 1, 0;
    %mov 4, 0, 1;
    %jmp/1 T_2.10, 4;
    %load/x1p 12, v0xae7cd0_0, 3;
    %jmp T_2.11;
T_2.10 ;
    %mov 12, 2, 3;
T_2.11 ;
    %mov 8, 12, 3; Move signal select into place
    %load/v 11, v0xae7cd0_0, 1; Select 1 out of 4 bits
    %ix/load 0, 4, 0;
    %assign/v0 v0xae7cd0_0, 0, 8;
    %ix/load 0, 1, 0;
    %assign/v0 v0xae80e0_0, 0, 0;
T_2.8 ;
T_2.5 ;
    %jmp T_2.3;
T_2.2 ;
    %load/v 8, v0xae7f60_0, 2;
    %cmpi/u 8, 0, 2;
    %jmp/0xz  T_2.12, 4;
    %load/v 8, v0xae8040_0, 1;
    %load/v 9, v0xae7cd0_0, 3; Select 3 out of 4 bits
    %ix/load 0, 4, 0;
    %assign/v0 v0xae7cd0_0, 0, 8;
    %ix/load 1, 3, 0;
    %mov 4, 0, 1;
    %jmp/1 T_2.14, 4;
    %load/x1p 8, v0xae7cd0_0, 1;
    %jmp T_2.15;
T_2.14 ;
    %mov 8, 2, 1;
T_2.15 ;
; Save base=8 wid=1 in lookaside.
    %ix/load 0, 1, 0;
    %assign/v0 v0xae80e0_0, 0, 8;
    %jmp T_2.13;
T_2.12 ;
    %load/v 8, v0xae7f60_0, 2;
    %cmpi/u 8, 1, 2;
    %jmp/0xz  T_2.16, 4;
    %ix/load 1, 3, 0;
    %mov 4, 0, 1;
    %jmp/1 T_2.18, 4;
    %load/x1p 12, v0xae7cd0_0, 1;
    %jmp T_2.19;
T_2.18 ;
    %mov 12, 2, 1;
T_2.19 ;
    %mov 8, 12, 1; Move signal select into place
    %load/v 9, v0xae7cd0_0, 3; Select 3 out of 4 bits
    %ix/load 0, 4, 0;
    %assign/v0 v0xae7cd0_0, 0, 8;
    %ix/load 0, 1, 0;
    %assign/v0 v0xae80e0_0, 0, 0;
T_2.16 ;
T_2.13 ;
T_2.3 ;
    %load/v 8, v0xae7f60_0, 2;
    %cmpi/u 8, 2, 2;
    %jmp/0xz  T_2.20, 4;
    %load/v 8, v0xacc5e0_0, 4;
    %ix/load 0, 4, 0;
    %assign/v0 v0xae7cd0_0, 0, 8;
    %ix/load 0, 1, 0;
    %assign/v0 v0xae80e0_0, 0, 0;
T_2.20 ;
T_2.0 ;
    %jmp T_2;
    .thread T_2;
    .scope S_0xad0d50;
T_3 ;
    %vpi_call 2 8 "$dumpfile", "resultado.vcd";
    %vpi_call 2 9 "$dumpvars";
    %vpi_call 2 10 "$display", "time\011 clk  enb  dir  sIn  modo    D       Q    sOut";
    %vpi_call 2 11 "$monitor", "%g\011    %b  %b    %b    %b    %b    %b    %b   %b", $time, v0xae8820_0, v0xae89c0_0, v0xae88f0_0, v0xae8ba0_0, v0xae8a90_0, v0xae86d0_0, v0xae87a0_0, v0xae8c70_0;
    %end;
    .thread T_3;
# The file index is used to find the file name in the following table.
:file_names 5;
