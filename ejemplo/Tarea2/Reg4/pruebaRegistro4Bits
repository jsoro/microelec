#! /usr/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0xad0d50 .scope module, "testbench" "testbench" 2 1;
 .timescale 0 0;
v0xae86d0_0 .net "D", 3 0, v0xae82c0_0; 1 drivers
v0xae87a0_0 .net "Q", 3 0, v0xae7cd0_0; 1 drivers
v0xae8820_0 .net "clk", 0 0, v0xae8360_0; 1 drivers
v0xae88f0_0 .net "dir", 0 0, v0xae8410_0; 1 drivers
